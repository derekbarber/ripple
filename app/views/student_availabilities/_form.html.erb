<%= simple_form_for(@student_availability) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :student_id %>
    <%= f.input :day_of_week %>
    <%= f.input :start_time %>
    <%= f.input :end_time %>
    <%= f.input :preferred_time %>
    <%= f.input :order_of_preference %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
